        "code": "function render(ctx, time, params, canvas) {\n  const { width, height } = canvas;\n  const S = Math.min(width, height);\n  const t = (time / 1000) * (params.speed || 2.0);\n  \n  // Clear canvas and draw sky\n  const skyGradient = ctx.createLinearGradient(0, 0, 0, height);\n  if (params.timeOfDay === 'day') {\n    skyGradient.addColorStop(0, '#5DADE2');\n    skyGradient.addColorStop(1, '#EBF5FB');\n  } else if (params.timeOfDay === 'sunset') {\n    skyGradient.addColorStop(0, '#2C3E50');\n    skyGradient.addColorStop(0.5, '#E74C3C');\n    skyGradient.addColorStop(1, '#F1C40F');\n  } else {\n    skyGradient.addColorStop(0, '#0B1021');\n    skyGradient.addColorStop(1, '#1B2735');\n  }\n  ctx.fillStyle = skyGradient;\n  ctx.fillRect(0, 0, width, height);\n  \n  // Celestial Body (Sun/Moon)\n  const celestialY = height * 0.25 + Math.sin(t * 0.2) * 10;\n  if (params.timeOfDay === 'day') {\n    ctx.fillStyle = '#F4D03F';\n    ctx.beginPath(); ctx.arc(width * 0.8, celestialY, S * 0.08, 0, Math.PI*2); ctx.fill();\n  } else if (params.timeOfDay === 'sunset') {\n    ctx.fillStyle = '#E67E22';\n    ctx.beginPath(); ctx.arc(width * 0.8, height * 0.4, S * 0.1, 0, Math.PI*2); ctx.fill();\n  } else {\n    ctx.fillStyle = '#FDFEFE';\n    ctx.beginPath(); ctx.arc(width * 0.8, celestialY, S * 0.06, 0, Math.PI*2); ctx.fill();\n    ctx.fillStyle = skyGradient; // Moon cutout for crescent\n    ctx.beginPath(); ctx.arc(width * 0.8 - S*0.015, celestialY - S*0.015, S * 0.05, 0, Math.PI*2); ctx.fill();\n  }\n\n  // Clouds\n  ctx.fillStyle = params.timeOfDay === 'night' ? 'rgba(255, 255, 255, 0.1)' : 'rgba(255, 255, 255, 0.7)';\n  for (let i = 0; i < 4; i++) {\n    let cx = (width + (i * width / 4) - ((t * 30 * (i % 2 + 1)) % (width * 2))) % (width * 2) - width/2;\n    let cy = height * 0.15 + (i * height * 0.08);\n    ctx.beginPath();\n    ctx.arc(cx, cy, S * 0.05, 0, Math.PI*2);\n    ctx.arc(cx + S * 0.04, cy - S * 0.02, S * 0.06, 0, Math.PI*2);\n    ctx.arc(cx + S * 0.08, cy + S * 0.01, S * 0.04, 0, Math.PI*2);\n    ctx.fill();\n  }\n\n  const groundY = height / 2 + S * 0.25;\n  \n  // Mountains\n  ctx.fillStyle = params.timeOfDay === 'day' ? '#A2D9CE' : (params.timeOfDay === 'sunset' ? '#8E44AD' : '#17202A');\n  ctx.beginPath();\n  ctx.moveTo(0, groundY);\n  ctx.lineTo(width * 0.15, groundY - S * 0.2);\n  ctx.lineTo(width * 0.35, groundY - S * 0.05);\n  ctx.lineTo(width * 0.65, groundY - S * 0.3);\n  ctx.lineTo(width * 0.85, groundY - S * 0.15);\n  ctx.lineTo(width, groundY - S * 0.25);\n  ctx.lineTo(width, groundY);\n  ctx.fill();\n\n  // Ground\n  ctx.fillStyle = params.timeOfDay === 'night' ? '#1C2833' : '#7F8C8D';\n  ctx.fillRect(0, groundY, width, height - groundY);\n\n  // Moving road lines\n  ctx.strokeStyle = params.timeOfDay === 'night' ? '#566573' : '#D0D3D4';\n  ctx.lineWidth = S * 0.015;\n  ctx.setLineDash([S * 0.1, S * 0.1]);\n  ctx.lineDashOffset = -(t * 300) % (S * 0.2);\n  ctx.beginPath();\n  ctx.moveTo(0, groundY + S * 0.08);\n  ctx.lineTo(width, groundY + S * 0.08);\n  ctx.stroke();\n  ctx.setLineDash([]);\n\n  // --- Bicycle & Pelican Local Coordinate System ---\n  ctx.save();\n  const scale = S / 300;\n  // Translate so the wheels (radius ~40) touch the groundY exactly\n  const wheelR = params.wheelSize || 40;\n  ctx.translate(width / 2, groundY - (wheelR * scale));\n  ctx.scale(scale, scale);\n\n  const rearX = -75, rearY = 0;\n  const frontX = 75, frontY = 0;\n  const wheelAngle = t * 6;\n\n  // Draw Wheel function\n  function drawWheel(x, y, angle) {\n    ctx.save();\n    ctx.translate(x, y);\n    ctx.rotate(angle);\n    // Tire\n    ctx.strokeStyle = '#212F3D';\n    ctx.lineWidth = 8;\n    ctx.beginPath(); ctx.arc(0, 0, wheelR, 0, Math.PI*2); ctx.stroke();\n    // Rim\n    ctx.strokeStyle = '#BDC3C7';\n    ctx.lineWidth = 2;\n    ctx.beginPath(); ctx.arc(0, 0, wheelR - 4, 0, Math.PI*2); ctx.stroke();\n    // Spokes\n    ctx.strokeStyle = '#95A5A6';\n    ctx.lineWidth = 1.5;\n    for(let i = 0; i < 10; i++) {