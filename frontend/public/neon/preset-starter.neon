tx.fill();\n\n  // Head\n  ctx.beginPath();\n  ctx.arc(headX, headY, 18, 0, Math.PI*2);\n  ctx.fill();\n\n  // Beak (Upper)\n  const beakColor = params.beakColor || '#F39C12';\n  ctx.fillStyle = beakColor;\n  ctx.beginPath();\n  ctx.moveTo(headX + 15, headY - 5);\n  ctx.lineTo(headX + 80, headY + 5);\n  ctx.lineTo(headX + 10, headY + 5);\n  ctx.fill();\n\n  // Pouch (Lower Beak)\n  ctx.fillStyle = '#F5B041';\n  ctx.beginPath();\n  ctx.moveTo(headX + 10, headY + 5);\n  ctx.quadraticCurveTo(headX + 45, headY + 45, headX + 75, headY + 5);\n  ctx.fill();\n  \n  // Pouch details (lines)\n  ctx.strokeStyle = 'rgba(211, 84, 0, 0.3)';\n  ctx.lineWidth = 1;\n  ctx.beginPath();\n  ctx.moveTo(headX + 20, headY + 5);\n  ctx.quadraticCurveTo(headX + 35, headY + 25, headX + 50, headY + 5);\n  ctx.stroke();\n\n  // Eye\n  ctx.fillStyle = '#000';\n  ctx.beginPath();\n  ctx.arc(headX + 8, headY - 6, 3, 0, Math.PI*2);\n  ctx.fill();\n  ctx.fillStyle = '#FFF';\n  ctx.beginPath();\n  ctx.arc(headX + 7, headY - 7, 1, 0, Math.PI*2);\n  ctx.fill();\n\n  // Helmet\n  ctx.fillStyle = params.bikeColor || '#E74C3C';\n  ctx.beginPath();\n  ctx.arc(headX, headY - 5, 19, Math.PI, Math.PI*2);\n  ctx.fill();\n  // Helmet rim\n  ctx.strokeStyle = '#C0392B';\n  ctx.lineWidth = 3;\n  ctx.beginPath();\n  ctx.moveTo(headX - 20, headY - 5);\n  ctx.lineTo(headX + 25, headY - 5);\n  ctx.stroke();\n  // Helmet strap\n  ctx.strokeStyle = '#2C3E50';\n  ctx.lineWidth = 2;\n  ctx.beginPath();\n  ctx.moveTo(headX - 10, headY - 5);\n  ctx.lineTo(headX - 2, headY + 16);\n  ctx.lineTo(headX + 12, headY - 5);\n  ctx.stroke();\n\n  // Scarf blowing in wind\n  ctx.strokeStyle = '#E74C3C';\n  ctx.lineWidth = 8;\n  ctx.lineCap = 'round';\n  ctx.beginPath();\n  ctx.moveTo(headX - 5, headY + 15);\n  ctx.quadraticCurveTo(bodyX - 30, headY + 20 + Math.sin(t*8)*10, bodyX - 60 - Math.cos(t*12)*5, headY + 10 + Math.sin(t*15)*8);\n  ctx.stroke();\n\n  // --- Left (Front) Side --- //\n  \n  // Left Leg (Front)\n  drawLeg(bodyX 